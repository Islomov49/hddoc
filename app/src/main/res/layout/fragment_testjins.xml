<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                xmlns:tools="http://schemas.android.com/tools"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:clickable="true"
                tools:context="com.isoma.homiladavridoctor.fragments.TestDateFragment"
    >


    <LinearLayout
        android:id="@+id/relativeLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="?attr/colorPrimaryLight"
        android:orientation="vertical"
        android:padding="0dp"
        >

        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginBottom="@dimen/sixty_dp">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/twenty_dp"
                android:padding="0dp"
                >

                <LinearLayout
                    android:id="@+id/frameLayout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="-10dp"
                    android:layout_marginLeft="0dp"
                    android:layout_marginRight="0dp"
                    android:layout_marginTop="0dp"
                    android:background="#fefefe"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">


                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:orientation="vertical">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text=""
                                android:gravity="bottom"
                                android:layout_marginLeft="@dimen/thirty_dp"
                                android:paddingTop="0dp"
                                android:paddingBottom="0dp"
                                android:textColor="#1f8397"
                                android:textSize="@dimen/thirty_dp"
                                android:textStyle="bold"
                                android:id="@+id/datee"
                                android:layout_marginTop="@dimen/ten_dp"
                                />
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="@dimen/thirty_dp"
                                android:textSize="@dimen/eighteen_dp"
                                android:text="@string/title_test"
                                android:id="@+id/moth"
                                android:textColor="#525153"
                                android:paddingRight="@dimen/ten_dp"/>

                        </LinearLayout>


                        <ImageView
                            android:layout_width="@dimen/hundred_dp"
                            android:layout_height="@dimen/hundred_fifty_dp"
                            android:src="@drawable/male"
                            android:id="@+id/iconaaa"
                            android:paddingTop="15dp"
                            android:paddingBottom="15dp"
                            android:layout_marginRight="@dimen/twenty_dp"
                            android:scaleType="centerInside"
                            />


                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/llLike"
                        android:visibility="gone"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:paddingBottom="@dimen/ten_dp"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/textView12"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:layout_gravity="center"
                            android:textColor="@color/main_color_gray_lt"
                            android:layout_marginLeft="@dimen/thirty_dp"
                            android:text="@string/is_it_right"/>

                        <LinearLayout
                            android:id="@+id/dislikeLayout"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:background="@drawable/rounded_rect"
                            android:gravity="center"
                            android:layout_gravity="center"
                            android:orientation="horizontal">

                            <TextView
                                android:id="@+id/dislike"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:padding="@dimen/five_dp"
                                android:textColor="@color/main_color_gray_lt"
                                android:layout_marginLeft="@dimen/ten_dp"
                                android:text="0"/>

                            <ImageView
                                android:id="@+id/dislikeImage"
                                android:layout_width="@dimen/twenty_five_dp"
                                android:layout_height="@dimen/twenty_five_dp"
                                android:padding="@dimen/five_dp"
                                android:layout_marginRight="@dimen/ten_dp"
                                android:layout_gravity="center"
                                android:src="@drawable/klass_down"/>

                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/likeLayout"

                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:background="@drawable/rounded_rect"
                            android:layout_marginLeft="@dimen/five_dp"
                            android:layout_marginRight="@dimen/sixteen_dp"
                            android:gravity="center"
                            android:layout_gravity="center"
                            android:orientation="horizontal">

                            <TextView
                                android:id="@+id/likeee"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:padding="@dimen/five_dp"
                                android:textColor="@color/main_color_gray_lt"
                                android:layout_marginLeft="@dimen/ten_dp"
                                android:text="0"/>

                            <ImageView
                                android:id="@+id/likeeeImage"

                                android:layout_width="@dimen/twenty_five_dp"
                                android:layout_height="@dimen/twenty_five_dp"
                                android:padding="@dimen/five_dp"
                                android:layout_marginRight="@dimen/ten_dp"
                                android:layout_gravity="center"
                                android:src="@drawable/klass"/>

                        </LinearLayout>

                    </LinearLayout>
                </LinearLayout>

                <TextView
                    android:id="@+id/yozu"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="38dp"
                    android:text="@string/preg_age"
                    android:textColor="#2c2d2e"
                    android:textSize="@dimen/eighteen_dp"
                    android:textStyle="bold"
                    android:layout_below="@+id/frameLayout"
                    android:layout_alignLeft="@+id/yozut2"
                    android:layout_alignStart="@+id/yozut2" />

                <TextView
                    android:id="@+id/ttt"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignEnd="@+id/seekyosh"
                    android:layout_alignRight="@+id/seekyosh"
                    android:layout_below="@id/yozu"
                    android:layout_marginTop="@dimen/five_dp"
                    android:gravity="center"
                    android:text=""
                    android:textColor="#1f8296"
                    android:textSize="@dimen/fifteen_dp"/>

                <SeekBar
                    android:id="@+id/seekyosh"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/ttt"
                    android:layout_marginBottom="0dp"
                    android:layout_marginLeft="@dimen/sixteen_dp"
                    android:layout_marginRight="@dimen/sixteen_dp"
                    android:layout_marginTop="0dp"
                    android:max="27"
                    android:progress="0"/>


                <TextView
                    android:id="@+id/yozut2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/seekyosh"
                    android:layout_marginLeft="@dimen/thirty_dp"
                    android:layout_marginTop="@dimen/twenty_dp"
                    android:text="@string/preg_month"
                    android:textColor="#2b2c2d"
                    android:textSize="@dimen/eighteen_dp"
                    android:textStyle="bold"
                    />

                <TextView
                    android:id="@+id/oyi"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignEnd="@+id/seekyosh"
                    android:layout_alignRight="@+id/seekyosh"
                    android:layout_below="@id/yozut2"
                    android:layout_marginTop="@dimen/five_dp"
                    android:gravity="center"
                    android:text=""
                    android:textColor="#1f8296"
                    android:textSize="@dimen/fifteen_dp"/>

                <SeekBar
                    android:id="@+id/seekoy"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/oyi"
                    android:layout_marginBottom="@dimen/twenty_dp"
                    android:layout_marginLeft="@dimen/sixteen_dp"
                    android:layout_marginRight="@dimen/sixteen_dp"
                    android:layout_marginTop="0dp"
                    android:max="11"
                    android:progress="0"
                    />

                <FrameLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/twenty_dp"
                    android:layout_below="@id/seekoy"/>
            </RelativeLayout>
        </ScrollView>


    </LinearLayout>


    <TextView
        android:id="@+id/hisoblasiin"
        android:layout_width="match_parent"
        android:layout_height="@dimen/sixty_dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="@dimen/five_dp"
        android:layout_marginRight="@dimen/ten_dp"
        android:background="@color/drfio"
        android:gravity="center"
        android:text="@string/calculate"
        android:textColor="#fff"
        android:textSize="@dimen/twenty_dp"
        android:visibility="gone"/>
</RelativeLayout>